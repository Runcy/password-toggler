{
	"manifest_version": 2,
	"name": "Password Toggler - view typed passwords",
	"short_name": "passTogg",
	"description": "Adds button to show/hide password in input elements",
	"version": "1.3",
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"main.js"
			],
			"css": [
				"main.css"
			],
			"run_at": "document_start"
		}
	]
}
