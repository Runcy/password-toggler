{
	"manifest_version": 2,
	"name": "Password Toggler - view typed passwords",
	"short_name": "passTogg",
	"description": "Adds button to show password in web pages",
	"version": "1.1",
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["content-script.js"]
	}]
}
