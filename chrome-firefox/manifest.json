{
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "css": [
                "main.css"
            ],
            "js": [
                "main.js"
            ],
            "matches": [
                "*://*/*"
            ],
            "run_at": "document_start"
        }
    ],
    "description": "Adds button to show/hide password in input elements",
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2,
    "name": "Password Toggler - view typed passwords",
    "short_name": "passTogg",
    "version": "1.3.1"
}
